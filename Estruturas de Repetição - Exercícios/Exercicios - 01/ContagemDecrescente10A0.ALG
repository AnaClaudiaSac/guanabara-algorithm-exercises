Algoritmo "contagemDecrescenteDe10A0"

Var
   contador: inteiro

Inicio
   //Contagem Decrescente:
   //Elabore um programa que mostre os números de 10 a 1 em ordem decrescente.
   contador <- 10
   Enquanto (contador >= 0) faca
      Escreval (contador)
      contador <- contador - 1
   FimEnquanto
   Escreval("Terminei de contar")

Fimalgoritmo